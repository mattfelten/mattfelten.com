    "gatsby-transformer-sharp": "2.8.0",
    "mdx-utils": "^0.2.0",
    "prism-react-renderer": "1.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-live": "2.2.3",
    "simple-icons-react-component": "1.0.0",
    "styled-components": "5.2.1",
    "typescript": "4.1.2",
    "use-dark-mode": "2.3.1"
