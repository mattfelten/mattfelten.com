    "classnames": "2.2.6",
    "date-fns": "2.14.0",
    "gatsby": "2.24.1",
    "gatsby-image": "2.4.13",
    "gatsby-plugin-feed": "2.5.11",
    "gatsby-plugin-google-analytics": "2.3.11",
    "gatsby-plugin-manifest": "2.4.18",
    "gatsby-plugin-mdx": "1.2.25",
