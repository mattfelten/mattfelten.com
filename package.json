    "gatsby-transformer-sharp": "2.5.11",
    "mdx-utils": "^0.2.0",
    "prism-react-renderer": "1.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-live": "^2.2.0",
    "simple-icons-react-component": "1.0.0",
    "styled-components": "5.1.1",
    "typescript": "3.9.6"
