    "classnames": "2.2.6",
    "date-fns": "2.16.1",
    "gatsby": "2.28.1",
    "gatsby-image": "2.7.0",
    "gatsby-plugin-feed": "2.9.0",
    "gatsby-plugin-google-analytics": "2.7.0",
    "gatsby-plugin-manifest": "2.8.0",
    "gatsby-plugin-mdx": "1.6.0",
    "gatsby-plugin-offline": "3.6.0",
